      <string id="DisablePreviewBuildsAndOptOut">Slå eksempelbuilds fra, når den næste udgave er offentliggjort</string>
      <string id="EnablePreviewBuilds">Slå eksempelbuilds til</string>
      <string id="PreviewBuildFastFeatureUpdates">Eksempelbuild - Hurtig</string>
      <string id="PreviewBuildSlowFeatureUpdates">Eksempelbuild - Langsom</string>
      <string id="ReleasePreviewBranchFeatureUpdates">Release-eksempel</string>
